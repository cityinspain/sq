{
  "version": 1,
  "projects": {
    "baseball": {
      "projectType": "application",
      "root": "apps/baseball",
      "sourceRoot": "apps/baseball/src",
      "prefix": "sq",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/baseball",
            "index": "apps/baseball/src/index.html",
            "main": "apps/baseball/src/main.ts",
            "polyfills": "apps/baseball/src/polyfills.ts",
            "tsConfig": "apps/baseball/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/baseball/src/favicon.ico",
              "apps/baseball/src/assets"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/baseball/src/environments/environment.ts",
                  "with": "apps/baseball/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "baseball:build:production"
            },
            "development": {
              "browserTarget": "baseball:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "baseball:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/baseball/src/**/*.ts",
              "apps/baseball/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/baseball"],
          "options": {
            "jestConfig": "apps/baseball/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "baseball-e2e": {
      "root": "apps/baseball-e2e",
      "sourceRoot": "apps/baseball-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/baseball-e2e/cypress.json",
            "tsConfig": "apps/baseball-e2e/tsconfig.e2e.json",
            "devServerTarget": "baseball:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "baseball:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/baseball-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "gql-server": {
      "root": "apps/gql-server",
      "sourceRoot": "apps/gql-server/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/gql-server",
            "main": "apps/gql-server/src/main.ts",
            "tsConfig": "apps/gql-server/tsconfig.app.json",
            "assets": ["apps/gql-server/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/gql-server/src/environments/environment.ts",
                  "with": "apps/gql-server/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "gql-server:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/gql-server/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/gql-server"],
          "options": {
            "jestConfig": "apps/gql-server/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "profile": {
      "projectType": "library",
      "root": "libs/profile",
      "sourceRoot": "libs/profile/src",
      "prefix": "sq",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/profile"],
          "options": {
            "jestConfig": "libs/profile/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/profile/src/**/*.ts",
              "libs/profile/src/**/*.html"
            ]
          }
        }
      }
    },
    "ui": {
      "projectType": "library",
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "prefix": "sq",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui"],
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ui/src/**/*.ts", "libs/ui/src/**/*.html"]
          }
        }
      }
    },
    "util": {
      "root": "libs/util",
      "sourceRoot": "libs/util/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/util/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/util"],
          "options": {
            "jestConfig": "libs/util/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "baseball"
}
